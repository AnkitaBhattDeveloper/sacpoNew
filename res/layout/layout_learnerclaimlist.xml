<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ga_Learner_claim_Row"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="1dp">

    <TextView
        android:id="@+id/lblClaimDate"
        android:layout_width="140dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center" android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/lblClaimMonth"
        android:layout_width="140dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center" android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/lblClaimYear"
        android:layout_width="140dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center" android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/lblClaimAmount"
        android:layout_width="160dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center" android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/lblDaysWorked"
        android:layout_width="120dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center" android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/lblLeaveCount"
        android:layout_width="120dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center" android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/lblAbsentCount"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center" android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/lblMstatus"
        android:layout_width="210dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center" android:gravity="center_horizontal" />
    <TextView
        android:id="@+id/lblMDate"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center" android:gravity="center_horizontal" />
    <TextView
        android:id="@+id/lblMAmmount"
        android:layout_width="210dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center" android:gravity="center_horizontal" />
    <TextView
        android:id="@+id/lblAdminAmmount"
        android:layout_width="210dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center" android:gravity="center_horizontal" />

    <TextView
        android:id="@+id/lblAproveDate"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center" android:gravity="center_horizontal" />


</TableRow>
