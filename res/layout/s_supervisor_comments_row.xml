<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/supervisor_row">
    <LinearLayout
        android:layout_marginTop="@dimen/SmallDp"
        android:layout_marginLeft="@dimen/SmallestDp"
        android:layout_marginRight="@dimen/SmallestDp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout
        android:id="@+id/head_container"
        android:weightSum="1"
        android:background="@color/colorPrimaryLight"
        android:orientation="horizontal"
        android:padding="@dimen/SmallDp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/get_user_name_c"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:ellipsize="end"
            android:gravity="left"
            android:textSize="@dimen/BigTextSize"
            android:textStyle="bold"
            android:textAllCaps="true"
            android:textColor="@color/colorPrimaryDark"
            android:text="zzz" />

    </LinearLayout>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:background="@color/background_s_divider"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/SmallerDp"
        android:elevation="5dp"
        android:layout_marginTop="@dimen/SmallerDp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/body_container"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/margin_5"
                android:orientation="vertical"
                >


                <TextView
                    android:id="@+id/lblRef"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:ellipsize="end"
                    android:gravity="left"
                    android:text="wwww" />



            </LinearLayout>

  <LinearLayout
                android:id="@+id/feedBack_container"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/margin_5"
                android:orientation="vertical"
                android:padding="@dimen/SmallDp">

      <TextView
          android:id="@+id/lblDate"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:textAlignment="viewEnd"
          android:textSize="@dimen/SmallerTextSize"
          android:gravity="end"
          android:text="aaa"
          />




            </LinearLayout>




        </LinearLayout>


    </androidx.cardview.widget.CardView>
    </LinearLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>