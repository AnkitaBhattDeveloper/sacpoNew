<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/newClaimRow"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="1dp">

    <TextView
        android:id="@+id/lblClaimYear3"
        android:layout_width="80dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:gravity="center_horizontal"
        android:textAlignment="center"  />
    <TextView
        android:id="@+id/lblClaimMonth4"
        android:layout_width="80dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:gravity="center_horizontal"
        android:textAlignment="center"  />
    <TextView
        android:id="@+id/lblDate5"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:gravity="center_horizontal"
        android:textAlignment="center"  />

    <TextView
        android:id="@+id/lblClaimAmount6"
        android:layout_width="165dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:gravity="center_horizontal"
        android:textAlignment="center"  />

    <TextView
        android:id="@+id/lblSupervisorStatus7"
        android:layout_width="190dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:gravity="center_horizontal"
        android:textAlignment="center"  />

    <TextView
        android:id="@+id/lblGrantAdminStatus8"
        android:layout_width="190dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:gravity="center_horizontal"
        android:textAlignment="center"  />

    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:layout_margin="1dp"
        android:textAlignment="center"
        android:layout_gravity="center_vertical|center_horizontal"
        android:id="@+id/linearAction"
        android:background="@drawable/backcell"
        >
        <TextView
            android:visibility="visible"
            android:id="@+id/lblAction"
            android:layout_width="300dp"
            android:textAlignment="center"
            android:gravity="center_horizontal"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:padding="@dimen/SmallerDp"
            android:ellipsize="end"
            android:maxLines="1"
            android:text=""/>
        <Button
            android:id="@+id/btnAction"
            style="?android:textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|center_vertical"
            android:layout_weight=".2"
            android:background="@drawable/themed_small_button"
            android:minHeight="@dimen/btnHeightSmall"
            android:minWidth="105dp"
            android:text=""
            android:textColor="@color/row_button_color"
            android:textSize="12sp"
            android:textStyle="bold"  />
    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearAction2"
        android:layout_width="250dp"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical|center_horizontal"
        android:layout_margin="1dp"
        android:orientation="horizontal"
        android:gravity="center"
        android:textAlignment="center" >

        <TextView
            android:id="@+id/lblAction2"
            android:layout_width="250dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:ellipsize="end"
            android:maxLines="1"
            android:padding="@dimen/SmallerDp"
            android:text=""
            android:textAlignment="center"
            android:gravity="center_horizontal"
            android:visibility="visible" />

        <Button
            android:id="@+id/btnAction2"
            style="?android:textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|center_vertical"
            android:layout_weight=".2"
            android:background="@drawable/themed_small_button"
            android:minHeight="@dimen/btnHeightSmall"
            android:minWidth="105dp"
            android:text=""
            android:textColor="@color/row_button_color"
            android:textSize="12sp"
            android:textStyle="bold" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearAction3"
        android:layout_width="260dp"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical|center_horizontal"
        android:layout_margin="1dp"
        android:orientation="horizontal"
        android:gravity="center"
        android:textAlignment="center" >

        <TextView
            android:id="@+id/lblAction3"
            android:layout_width="260dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:ellipsize="end"
            android:maxLines="1"
            android:padding="@dimen/SmallerDp"
            android:text=""
            android:textAlignment="center"
            android:gravity="center_horizontal"
            android:visibility="visible" />

        <Button
            android:id="@+id/btnAction3"
            style="?android:textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|center_vertical"
            android:layout_weight=".2"
            android:background="@drawable/themed_small_button"
            android:minHeight="@dimen/btnHeightSmall"
            android:minWidth="105dp"
            android:text=""
            android:textColor="@color/row_button_color"
            android:textSize="12sp"
            android:textStyle="bold" />

    </LinearLayout>

</TableRow>
