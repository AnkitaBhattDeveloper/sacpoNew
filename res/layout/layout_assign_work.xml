<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mAW_Row"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="1dp">

    <TextView
        android:id="@+id/lblLearnerName"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:background="@drawable/backcell"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center"
        android:visibility="visible" />

    <TextView
        android:id="@+id/lblEmail"
        android:layout_width="170dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:background="@drawable/backcell"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center"
        android:visibility="visible" />

    <TextView
        android:id="@+id/lblCellNo"
        android:layout_width="130dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:background="@drawable/backcell"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center"
        android:visibility="visible" />


    <TextView
        android:id="@+id/lblGrantName"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:background="@drawable/backcell"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center"
        android:visibility="visible" />

    <TextView
        android:id="@+id/lblStartDate"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:background="@drawable/backcell"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center"
        android:visibility="visible" />

    <TextView
        android:id="@+id/lblEndDate"
        android:layout_width="160dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:background="@drawable/backcell"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:textAlignment="center"
        android:visibility="visible" />



    <LinearLayout
        android:visibility="visible"
        android:id="@+id/linearLAction1"
        android:layout_width="200dp"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical|center_horizontal"
        android:layout_margin="1dp"
        android:orientation="horizontal"
        android:textAlignment="center" android:gravity="center_horizontal">

        <TextView

            android:id="@+id/lblAction1"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:ellipsize="end"
            android:maxLines="1"
            android:padding="@dimen/SmallerDp"
            android:text=""
            android:textAlignment="center" android:gravity="center_horizontal"
            android:visibility="gone" />

        <Button
            android:id="@+id/btnAction1"
            style="?android:textAppearanceSmall"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|center_vertical"
            android:layout_weight=".2"
            android:background="@drawable/themed_small_button"
            android:minHeight="@dimen/btnHeightSmall"
            android:minWidth="105dp"
            android:text=""
            android:textColor="@color/row_button_color"
            android:textSize="12sp"
            android:textStyle="bold" />

    </LinearLayout>


    <LinearLayout
        android:visibility="visible"
        android:id="@+id/linearLAction2"
        android:layout_width="160dp"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical|center_horizontal"
        android:layout_margin="1dp"
        android:orientation="horizontal"
        android:textAlignment="center" android:gravity="center_horizontal">

        <TextView

            android:id="@+id/lblAction2"
            android:layout_width="160dp"
            android:layout_height="match_parent"
            android:layout_margin="1dp"
            android:ellipsize="end"
            android:maxLines="1"
            android:padding="@dimen/SmallerDp"
            android:text=""
            android:textAlignment="center" android:gravity="center_horizontal"
            android:visibility="gone" />

        <Button
            android:id="@+id/btnAction2"
            style="?android:textAppearanceSmall"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|center_vertical"
            android:layout_weight=".2"
            android:background="@drawable/themed_small_button"
            android:minHeight="@dimen/btnHeightSmall"
            android:minWidth="105dp"
            android:text=""
            android:textColor="@color/row_button_color"
            android:textSize="12sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btnActionSecondBtn"
            android:visibility="gone"
            style="?android:textAppearanceSmall"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|center_vertical"
            android:layout_weight=".2"
            android:background="@drawable/themed_small_button"
            android:minHeight="@dimen/btnHeightSmall"
            android:minWidth="105dp"
            android:text=""
            android:textColor="@color/row_button_color"
            android:textSize="12sp"
            android:textStyle="bold" />

    </LinearLayout>

</TableRow>
