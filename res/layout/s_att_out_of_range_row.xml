<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tRowA"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="1dp"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <!--android:layout_width="100dp"-->
    <TextView
        android:id="@+id/lblLoginDate"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:background="@drawable/backcell"
        android:ellipsize="end"
        android:maxLines="1"
        android:textAlignment="center" android:gravity="center_horizontal"
        />

    <TextView
        android:id="@+id/lblDay"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:ellipsize="end"
        android:background="@drawable/backcell"
        android:maxLines="1"
        android:textAlignment="center" android:gravity="center_horizontal"
        />
    <TextView
        android:id="@+id/lblLoginTime"
        android:layout_width="170dp"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:minHeight="@dimen/btnHeightSmall"
        android:padding="@dimen/SmallerDp"
        android:text=""
        android:ellipsize="end"
        android:background="@drawable/backcell"
        android:maxLines="1"
        android:textAlignment="center" android:gravity="center_horizontal"
        />


</TableRow>